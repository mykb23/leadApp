doctype html
html(lang="en")
  include partials/head.pug
  +head("Starter MVP")
  body
    h1 Lead
    p 
        span #{lead.email}
        a(href="/lead/" + lead.id + "/edit") Edit
        | 
        form(action="/lead/" + lead.id + "/delete", method="post")
            button(type="submit") Delete
    include partials/footer.pug
    +footer()
